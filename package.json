{
  "name": "typescript-node-api",
  "version": "0.0.0",
  "description": "A simple boilerplate typescript node.js api",
  "main": "lib/server.js",
  "typings": "lib/server.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node ./src/server.ts",
    "start": "node ./lib/server.js",
    "prod": "npm run build && cross-env NODE_ENV=production npm run start"
  },
  "author": "Daniel Verdejo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Verdagio/typescript-node-api.git"
  },
  "keywords": [
    "node",
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Verdagio/typescript-node-api/issues"
  },
  "homepage": "https://github.com/Verdagio/typescript-node-api#readme",
  "devDependencies": {
    "@types/assert": "0.0.31",
    "@types/bunyan": "^1.8.3",
    "@types/config": "0.0.33",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.4",
    "cross-env": "^5.0.5",
    "nodemon": "^1.17.5",
    "ts-node": "^6.1.1",
    "tslint": "^5.7.0",
    "typescript": "^2.9.2",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
